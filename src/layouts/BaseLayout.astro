---
import MainHead from '../components/MainHead.astro';

type Props = {
  title: string;
  description: string;
};

const { title, description } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <MainHead title={title} description={description} />
  </head>
  <body>
    <slot />
  </body>
</html>
